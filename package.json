{
  "name": "pointnetwork-dashboard",
  "author": "Point Labs",
  "version": "0.2.0",
  "description": "PointNetwork",
  "main": "./.webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "electron-forge publish",
    "lint": "eslint . --ext js,ts",
    "test": "jest"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.17.5",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.4.3",
    "@mui/styled-engine-sc": "^5.4.2",
    "@types/tar-fs": "^2.0.1",
    "7zip-min": "^1.4.1",
    "arweave-mnemonic-keys": "0.0.9",
    "axios": "^0.26.0",
    "bitcore-lib": "^8.25.25",
    "bitcore-mnemonic": "^8.25.25",
    "dmg": "^0.1.0",
    "docker-compose": "^0.23.17",
    "find-process": "^1.4.7",
    "fs-extra": "10.0.1",
    "i": "^0.3.7",
    "isomorphic-git": "^1.12.1",
    "lodash": "^4.17.21",
    "npm": "^8.5.2",
    "pify": "^5.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-loader": "4.13.0",
    "react-icons": "^4.3.1",
    "simple-git": "^3.2.6",
    "styled-components": "^5.3.3",
    "tar-fs": "^2.1.1",
    "unbzip2-stream": "^1.4.3",
    "web-ext": "^6.7.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.17.2",
    "@babel/plugin-transform-runtime": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@electron-forge/cli": "6.0.0-beta.63",
    "@electron-forge/maker-deb": "6.0.0-beta.63",
    "@electron-forge/maker-rpm": "6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "6.0.0-beta.63",
    "@electron-forge/maker-zip": "6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
    "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
    "@nuxt/friendly-errors-webpack-plugin": "^2.5.2",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@types/7zip-min": "^1.3.1",
    "@types/electron-devtools-installer": "2.2.1",
    "@types/follow-redirects": "^1.14.1",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.178",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.23",
    "@types/which": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "autoprefixer": "^10.4.2",
    "babel-loader": "8.2.3",
    "cross-env": "7.0.3",
    "css-loader": "^6.6.0",
    "electron": "17.1.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-standard": "5.0.0",
    "file-loader": "^6.2.0",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.7",
    "postcss-loader": "^6.2.1",
    "prettier": "2.5.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.23",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5",
    "wait-on": "6.0.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "pointnetwork-dashboard",
        "executableName": "pointnetwork-dashboard",
        "icon": "assets/icon",
        "extraResource": [
          "assets"
        ]
      },
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack/main.webpack.js",
            "renderer": {
              "config": "./webpack/renderer.webpack.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/dashboard/ui/index.tsx",
                  "name": "dashboard_window",
                  "preload": {
                    "js": "./src/dashboard/bridge.ts"
                  }
                },
                {
                  "html": "./public/index.html",
                  "js": "./src/welcome/ui/index.tsx",
                  "name": "welcome_window",
                  "preload": {
                    "js": "./src/welcome/bridge.ts"
                  }
                },
                {
                  "html": "./public/index.html",
                  "js": "./src/installer/ui/index.tsx",
                  "name": "installer_window",
                  "preload": {
                    "js": "./src/installer/bridge.ts"
                  }
                },
                {
                  "html": "./public/index.html",
                  "js": "./src/dockerLog/ui/index.tsx",
                  "name": "docker_log_window",
                  "preload": {
                    "js": "./src/dockerLog/bridge.ts"
                  }
                }
              ]
            }
          }
        ]
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "pointnetwork-dashboard"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
