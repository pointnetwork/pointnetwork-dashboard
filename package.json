{
  "name": "pointnetwork-dashboard",
  "version": "1.0.0",
  "description": "PointNetwork Dashboard",
  "main": "app/main.js",
  "scripts": {
    "start": "node build/start.js",
    "forge-start": "electron-forge start",
    "forge-package": "electron-forge package",
    "forge-make": "electron-forge make"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-dmg": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "^15.3.0"
  },
  "dependencies": {
    "7zip-min": "^1.4.0",
    "@babel/core": "^7.16.0",
    "arweave-mnemonic-keys": "0.0.9",
    "axios": "^0.24.0",
    "babel-loader": "^8.2.3",
    "bip39": "^3.0.4",
    "child_process": "^1.0.2",
    "dmg": "^0.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "follow-redirects": "^1.14.5",
    "fs-extra": "^10.0.0",
    "isomorphic-git": "^1.10.1",
    "lodash": "^4.17.21",
    "pify": "^5.0.0",
    "simple-git": "^2.47.0",
    "sudo-prompt": "^9.2.1",
    "tar-fs": "^2.1.1",
    "unbzip2-stream": "^1.4.3",
    "web-ext": "^6.5.0",
    "webpack": "^5.62.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0",
    "which": "^2.0.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "pointnetwork_dashboard"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "background": "./resources/pointbackground.png",
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
